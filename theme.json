{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "768px",
			"wideSize": "1280px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Rubik, -apple-system, BlinkMacSystemFont, 'Segoe UI', Ubuntu, Roboto, Oxygen-Sans, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Rubik",
					"slug": "rubik",
					"fontFace": [
						{
							"fontFamily": "Rubik",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/rubik/rubik-variable.woff2"
							]
						},
						{
							"fontFamily": "Rubik",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/rubik/rubik-italic-variable.woff2"
							]
						}
					]
				}
			],
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"slug": "tiny",
					"size": ".75rem",
					"name": "Tiny"
				},
				{
					"slug": "small",
					"size": ".875rem",
					"name": "Small",
					"fluid": {
						"min": ".75rem",
						"max": ".875rem"
					}
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium",
					"fluid": {
						"min": ".875rem",
						"max": "1rem"
					}
				},
				{
					"slug": "large",
					"size": "1.25rem",
					"name": "Large",
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "heading-4",
					"size": "1.625rem",
					"name": "Heading 4",
					"fluid": {
						"min": "1.5rem",
						"max": "1.625rem"
					}
				},
				{
					"slug": "heading-3",
					"size": "1.875rem",
					"name": "Heading 3",
					"fluid": {
						"min": "1.625rem",
						"max": "1.875rem"
					}
				},
				{
					"slug": "heading-2",
					"size": "2.5rem",
					"name": "Heading 2",
					"fluid": {
						"min": "1.875rem",
						"max": "2.5rem"
					}
				},
				{
					"slug": "heading-1",
					"size": "3rem",
					"name": "Heading 1",
					"fluid": {
						"min": "2.5rem",
						"max": "3rem"
					}
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#141412",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#3b3b3b",
					"name": "Dark grey",
					"slug": "grey-4"
				},
				{
					"color": "#808080",
					"name": "Grey",
					"slug": "grey-3"
				},
				{
					"color": "#bfbfbf",
					"name": "Soft grey",
					"slug": "grey-2"
				},
				{
					"color": "#e0e0e0",
					"name": "Light grey",
					"slug": "grey-1"
				},
				{
					"color": "#fafafa",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#cc3d00",
					"name": "Darkest orange",
					"slug": "orange-5"
				},
				{
					"color": "#e95b02",
					"name": "Dark orange",
					"slug": "orange-4"
				},
				{
					"color": "#ff7033",
					"name": "Orange",
					"slug": "orange-3"
				},
				{
					"color": "#ffa782",
					"name": "Soft orange",
					"slug": "orange-2"
				},
				{
					"color": "#ffdbcc",
					"name": "Light orange",
					"slug": "orange-1"
				},
				{
					"color": "#fff4f0",
					"name": "Lightest orange",
					"slug": "orange-0"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(to bottom, transparent 50%, var(--wp--preset--color--contrast))",
					"name": "Text overlay - bottom",
					"slug": "text-overlay-bottom"
				}
			],
			"duotone": [
				{
					"colors": [
						"#141412",
						"#cc3d00"
					],
					"name": "Dark Orange",
					"slug": "dark-orange"
				},
				{
					"colors": [
						"#141412",
						"#ffa782"
					],
					"name": "Soft Orange",
					"slug": "soft-orange"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|rubik",
			"fontSize": "var:preset|font-size|medium",
			"fontWeight": "400",
			"lineHeight": "1.6"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.2",
					"letterSpacing": "1px"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"textTransform": "uppercase"
				}
			}
		},
		"blocks": {
			"core/quote": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var:preset|font-size|small"
						},
						"css": "text-align: right"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}