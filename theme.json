{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "768px",
			"wideSize": "1280px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": ".25rem",
					"slug": "10"
				},
				{
					"name": "extra small",
					"size": ".5rem",
					"slug": "20"
				},
				{
					"name": "Very small",
					"size": ".75rem",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "2rem",
					"slug": "60"
				},
				{
					"name": "Very large",
					"size": "3rem",
					"slug": "70"
				},
				{
					"name": "Extra large",
					"size": "4rem",
					"slug": "80"
				},
				{
					"name": "Huge",
					"size": "6rem",
					"slug": "90"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Rubik, -apple-system, BlinkMacSystemFont, 'Segoe UI', Ubuntu, Roboto, Oxygen-Sans, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Rubik",
					"slug": "rubik",
					"fontFace": [
						{
							"fontFamily": "Rubik",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/rubik/rubik-variable.woff2"
							]
						},
						{
							"fontFamily": "Rubik",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/rubik/rubik-italic-variable.woff2"
							]
						}
					]
				}
			],
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"slug": "tiny",
					"size": ".75rem",
					"name": "Tiny"
				},
				{
					"slug": "small",
					"size": ".875rem",
					"name": "Small",
					"fluid": {
						"min": ".75rem",
						"max": ".875rem"
					}
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium",
					"fluid": {
						"min": ".875rem",
						"max": "1rem"
					}
				},
				{
					"slug": "large",
					"size": "1.25rem",
					"name": "Large",
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "heading-4",
					"size": "1.625rem",
					"name": "Heading 4",
					"fluid": {
						"min": "1.5rem",
						"max": "1.625rem"
					}
				},
				{
					"slug": "heading-3",
					"size": "1.875rem",
					"name": "Heading 3",
					"fluid": {
						"min": "1.625rem",
						"max": "1.875rem"
					}
				},
				{
					"slug": "heading-2",
					"size": "2.5rem",
					"name": "Heading 2",
					"fluid": {
						"min": "1.875rem",
						"max": "2.5rem"
					}
				},
				{
					"slug": "heading-1",
					"size": "3rem",
					"name": "Heading 1",
					"fluid": {
						"min": "2.5rem",
						"max": "3rem"
					}
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#141412",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#3b3b3b",
					"name": "Dark grey",
					"slug": "grey-4"
				},
				{
					"color": "#808080",
					"name": "Grey",
					"slug": "grey-3"
				},
				{
					"color": "#bfbfbf",
					"name": "Soft grey",
					"slug": "grey-2"
				},
				{
					"color": "#e0e0e0",
					"name": "Light grey",
					"slug": "grey-1"
				},
				{
					"color": "#fafafa",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#cc3d00",
					"name": "Darkest orange",
					"slug": "orange-5"
				},
				{
					"color": "#e95b02",
					"name": "Dark orange",
					"slug": "orange-4"
				},
				{
					"color": "#ff7033",
					"name": "Orange",
					"slug": "orange-3"
				},
				{
					"color": "#ffa782",
					"name": "Soft orange",
					"slug": "orange-2"
				},
				{
					"color": "#ffdbcc",
					"name": "Light orange",
					"slug": "orange-1"
				},
				{
					"color": "#fff4f0",
					"name": "Lightest orange",
					"slug": "orange-0"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(to bottom, transparent 50%, var(--wp--preset--color--contrast))",
					"name": "Text overlay - bottom",
					"slug": "text-overlay-bottom"
				}
			],
			"duotone": [
				{
					"colors": [
						"#141412",
						"#cc3d00"
					],
					"name": "Dark Orange",
					"slug": "dark-orange"
				},
				{
					"colors": [
						"#141412",
						"#ffa782"
					],
					"name": "Soft Orange",
					"slug": "soft-orange"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Short",
					"slug": "short",
					"shadow": "0px 6px 9px rgba(20, 20, 18, 0.2)"
				},
				{
					"name": "Large",
					"slug": "large",
					"shadow": "0px 12px 50px rgba(20, 20, 18, 0.4)"
				}
			]
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var:preset|spacing|50",
			"padding": {
				"bottom": "0",
				"left": "var:preset|spacing|50",
				"right": "var:preset|spacing|50",
				"top": "0"
			}
		},
		"color": {
			"background": "var:preset|color|base",
			"text": "var:preset|color|contrast"
		},
		"typography": {
			"fontFamily": "var:preset|font-family|rubik",
			"fontSize": "var:preset|font-size|medium",
			"fontWeight": "400",
			"lineHeight": "1.6"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var:preset|color|orange-4"
				},
				":hover": {
					"color": {
						"text": "var:preset|color|orange-5"
					}
				},
				":focus": {
					"color": {
						"text": "var:preset|color|orange-5"
					},
					"outline": {
						"color": "var:preset|color|orange-4",
						"offset": "2px",
						"style": "dotted",
						"width": "2px"
					}
				}
			},
			"button": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"textTransform": "uppercase",
					"letterSpacing": ".5px",
					"fontWeight": "bold",
					"lineHeight": "1"
				},
				"color": {
					"background": "var:preset|color|orange-5",
					"text": "var:preset|color|base"
				},
				"css": "transition: all .25s ease-in-out",
				":focus": {
					"outline": {
						"color": "var:preset|color|orange-4",
						"offset": "2px",
						"style": "dotted",
						"width": "2px"
					}
				},
				":hover": {
					"color": {
						"background": "var:preset|color|orange-4",
						"text": "var:preset|color|base"
					},
					"border": {
						"color": "var:preset|color|orange-4"
					},
					"shadow": "var:preset|shadow|short"
				},
				"border": {
					"radius": "0",
					"width": "2px",
					"color": "var:preset|color|orange-5",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"bottom": "var:preset|spacing|30",
						"left": "var:preset|spacing|40",
						"right": "var:preset|spacing|40",
						"top": "var:preset|spacing|30"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.2",
					"letterSpacing": "1px"
				},
				"spacing": {
					"margin": {
						"top": "1.5em",
						"bottom": "var:preset|spacing|50"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|heading-4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"textTransform": "uppercase"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"textAlign": "center",
					"fontStyle": "italic"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "1.5em",
						"bottom": "var:preset|spacing|50"
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				},
				"elements": {
					"cite": {
						"css": "text-align: right"
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"color": {
							"text": "var:preset|color|orange-5"
						},
						"border": {
							"color": "currentColor"
						},
						"spacing": {
							"padding": {
								"bottom": "var:preset|spacing|30",
								"left": "var:preset|spacing|40",
								"right": "var:preset|spacing|40",
								"top": "var:preset|spacing|30"
							}
						},
						"css": ".wp-block-button__link:not(.has-background):hover {color: var(--wp--preset--color--orange-4)}"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}