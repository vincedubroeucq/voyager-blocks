	"styles": {
		"blocks": {
			"core/avatar": {
				"border": {
					"radius": "50px"
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor"
						},
						"color": {
							"text": "var(--wp--preset--color--orange-5)"
						},
						"css": ".wp-block-button__link:not(.has-background):hover {color: var(--wp--preset--color--orange-4)}",
						"spacing": {
							"padding": {
								"bottom": "var(--wp--preset--spacing--30)",
								"left": "var(--wp--preset--spacing--40)",
								"right": "var(--wp--preset--spacing--40)",
								"top": "var(--wp--preset--spacing--30)"
							}
						}
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--grey-1)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--50)",
						"top": "1.5em"
					}
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"css": "text-align: right"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				":focus": {
					"outline": {
						"color": "var(--wp--preset--color--orange-4)",
						"offset": "2px",
						"style": "dotted",
						"width": "2px"
					}
				},
				":hover": {
					"border": {
						"color": "var(--wp--preset--color--orange-4)"
					},
					"color": {
						"background": "var(--wp--preset--color--orange-4)",
						"text": "var(--wp--preset--color--base)"
					},
					"shadow": "var(--wp--preset--shadow--short)"
				},
				"border": {
					"color": "var(--wp--preset--color--orange-5)",
					"radius": "0",
					"style": "solid",
					"width": "2px"
				},
				"color": {
					"background": "var(--wp--preset--color--orange-5)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "transition: all .25s ease-in-out",
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "bold",
					"letterSpacing": ".5px",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "italic",
					"textAlign": "center"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--50)",
						"top": "1.5em"
					}
				},
				"typography": {
					"fontWeight": "700",
					"letterSpacing": "1px",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--orange-5)"
					},
					"outline": {
						"color": "var(--wp--preset--color--orange-4)",
						"offset": "2px",
						"style": "dotted",
						"width": "2px"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--orange-5)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--orange-4)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)",
			"padding": {
				"bottom": "0",
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--rubik)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.8"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
